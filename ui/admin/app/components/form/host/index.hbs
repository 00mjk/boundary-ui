{{#if @model.compositeType}}
  {{component
    (concat 'form/host/' @model.compositeType)
    model=@model
    submit=@submit
    cancel=@cancel
  }}
{{else}}
{{{log @model}}} modellllll
  {{component
    (concat 'form/host/' 'static')
    model=@model
    submit=@submit
    cancel=@cancel
  }}
{{/if}}