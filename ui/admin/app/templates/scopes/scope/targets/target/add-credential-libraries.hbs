{{page-title (t 'resources.target.actions.add-credential-libraries')}}

{{#if @model.credentialLibraries}}
  <Form::Target::AddCredentialLibraries
    @model={{@model.target}}
    @credentialLibraries={{@model.credentialLibraries}}
    @submit={{route-action 'save' @model.target}}
    @cancel={{route-action 'cancel'}}
  />
{{/if}}