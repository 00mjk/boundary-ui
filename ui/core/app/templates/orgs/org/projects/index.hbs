<div class="projects-overview">
  <h2>{{t "resources.projects"}}</h2>

  <LinkTo @route="orgs.org.projects.new" class="project-new-link">{{t "titles.new"}}</LinkTo>

  <ul class="projects">
    {{#each @model as |project|}}
      <li class="project">
        <LinkTo @route="orgs.org.projects.project" @model={{project}}>
          {{project.name}}
        </LinkTo>
      </li>
    {{/each}}
  </ul>
</div>
